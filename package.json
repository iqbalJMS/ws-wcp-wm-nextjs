{
  "name": "aether",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9",
    "bun": "please-use-npm",
    "yarn": "please-use-npm"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --write src",
    "checker": "node server/fs-checker.js",
    "dragon": "node server/dragon-runner.js",
    "obfuscate": "next build; node server/obfuscator.js",
    "purger": "node server/cache-purger.js",
    "dev:concurrent": "concurrently \"npm:dev -- --port 1223\" \"npm:dragon\"",
    "prod:concurrent": "concurrently \"npm:start -- --port 1223\" \"npm:dragon\" \"npm:purger\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@strix/client": "file:./node_build/strix-client-0.0.27.tgz",
    "@strix/server": "file:./node_build/strix-server-0.0.39.tgz",
    "aos": "^2.3.4",
    "framer-motion": "^11.5.4",
    "merge-refs": "^1.3.0",
    "motion": "^11.13.1",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18",
    "remarkable": "^2.0.1",
    "sharp": "^0.33.5",
    "tailwind-variants": "^0.2.1",
    "typescript": "^5.5.4"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.0.0",
    "elastic-apm-node": "^4.5.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
